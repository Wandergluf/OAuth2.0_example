<cn_unit> = "一" {nf.I(1)}
          | "二" {nf.I(2)}
          | "三" {nf.I(3)}
          | "四" {nf.I(4)}
          | "五" {nf.I(5)}
          | "六" {nf.I(6)}
          | "七" {nf.I(7)}
          | "八" {nf.I(8)}
          | "九" {nf.I(9)}
          | "壹" {nf.I(1)}
          | "贰" {nf.I(2)}
          | "叁" {nf.I(3)}
          | "肆" {nf.I(4)}
          | "伍" {nf.I(5)}
          | "陆" {nf.I(6)}
          | "柒" {nf.I(7)}
          | "捌" {nf.I(8)}
          | "玖" {nf.I(9)}
          | "两" {nf.I(2)}
          | "俩" {nf.I(2)}
          | "仨" {nf.I(3)}
          ;

<cn_zero> = "零" {nf.I(0)}
          | "〇" {nf.I(0)}
          ;

<cn_digit> = <cn_unit> {nf.I($1)} | <cn_zero> {nf.I($1)} | <digits> {nf.I($1)};

<numbers> = <cn_digit> {nf.I($1)}
          | <cn_digit> <numbers> {nf.util.concat($1, $2)}
          ;

<cn_e1> = "十" {nf.I(10)}
   